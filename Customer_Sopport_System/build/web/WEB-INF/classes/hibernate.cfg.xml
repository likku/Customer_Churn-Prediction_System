<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/feedbacksystemonproducts1</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="fs/EmployeeData.hbm.xml"/>
    <mapping resource="fs/PmDetails.hbm.xml"/>
    <mapping resource="fs/PackagesData.hbm.xml"/>
    <mapping resource="fs/UserDetails.hbm.xml"/>
    <mapping resource="fs/CompanyheadData.hbm.xml"/>
    <mapping resource="fs/OrgPackages.hbm.xml"/>
    <mapping resource="fs/ProductDetails.hbm.xml"/>
    <mapping resource="fs/FeedbackData.hbm.xml"/>
    <mapping resource="fs/Answersdata.hbm.xml"/>
    <mapping resource="fs/AdminData.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
