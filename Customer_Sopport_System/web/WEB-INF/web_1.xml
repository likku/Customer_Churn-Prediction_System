<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AdminLoginAction</servlet-name>
        <servlet-class>AdminLoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PMLoginAction</servlet-name>
        <servlet-class>PMLoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLoginAction</servlet-name>
        <servlet-class>UserLoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegAction</servlet-name>
        <servlet-class>UserRegAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PMRegAction</servlet-name>
        <servlet-class>PMRegAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginAction</servlet-name>
        <url-pattern>/AdminLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PMLoginAction</servlet-name>
        <url-pattern>/PMLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLoginAction</servlet-name>
        <url-pattern>/UserLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegAction</servlet-name>
        <url-pattern>/UserRegAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PMRegAction</servlet-name>
        <url-pattern>/PMRegAction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
